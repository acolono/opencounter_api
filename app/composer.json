{
    "name": "acolono/slim-opencounter",
    "description": "A Slim Framework implementation of opencounter",
    "keywords": ["microframework","rest","router", "psr7"],
    "homepage": "http://github.com/acolono/slim-opencounter",
    "license": "MIT",
  "version": "0.0.1-dev",
    "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/acolono/opencounter_core.git"
    }
  ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "3.x-dev",
        "slim/php-view": "^2.0",
      "slim/twig-view": "^2",
        "monolog/monolog": "^1.17",
        "zircote/swagger-php": "~2.0.7",
        "raveren/kint": "^1.0",
        "ramsey/uuid": "^3.5",
        "tuupola/cors-middleware": "^0.5.2",
        "robmorgan/phinx": "^0.6.4",
      "rosenstrauch/opencounter_api_core": "^0",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
      "behat/behat": "^3",
      "behat/web-api-extension": "^1",
      "pavlakis/slim-behat-extension": "^1",
        "phpspec/phpspec": "3",
        "phpunit/phpunit": "^5.4",
      "emuse/behat-html-formatter": "0.1.*",
        "behat/mink": "^1.7",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-extension": "^2.2"
    },
  "config": {
    "preferred-install": "dist",
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "SlimCounter\\": "src/"
    }
  }
}
