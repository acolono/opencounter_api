{
  "name": "acolono/slim-opencounter",
  "description": "A Slim Framework implementation of opencounter",
  "version": "1.1.0",
  "keywords": [
    "microframework",
    "rest",
    "router",
    "psr7"
  ],
  "homepage": "http://github.com/acolono/slim-opencounter",
  "license": "MIT",
  "minimum-stability": "dev",
  "repositories": [

    {
      "type": "vcs",
      "url": "git@github.com:rstgroup/behat-oauth2-context.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/acolono/opencounter_core.git"
    },
    {
      "type": "git",
      "url": "https://github.com/acolono/opencounter_theme.git"
    }

  ],
  "require": {
    "php": ">=5.5.0",
    "slim/slim": "3.x-dev",
    "slim/php-view": "^2.0",
    "slim/twig-view": "^2",
    "monolog/monolog": "^1.17",
    "zircote/swagger-php": "~2.0.7",
    "ramsey/uuid": "^3.5",
    "robmorgan/phinx": "^0.6.4",
    "slimcounter/default_theme": "^1.0",
    "tuupola/cors-middleware": "^0.5.2",
    "rosenstrauch/opencounter_api_core": "^0.2.4",
    "vlucas/phpdotenv": "^2.4",
    "chadicus/slim-oauth2": "^3.1",
    "tuupola/slim-basic-auth": "^2.2"
  },
  "require-dev": {
    "behat/behat": "^3",
    "behat/web-api-extension": "^1",
    "pavlakis/slim-behat-extension": "^1",
    "phpspec/phpspec": "3",
    "phpunit/phpunit": "^5.4",
    "emuse/behat-html-formatter": "0.1.*",
    "behat/mink": "^1.7",
    "behat/mink-goutte-driver": "^1.2",
    "rstgroup/behat-oauth2-context": "1.1.0",
    "behat/mink-extension": "^2.2"
  },
  "config": {
    "preferred-install": "dist",
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "SlimCounter\\": "src/"
    }
  }
}
